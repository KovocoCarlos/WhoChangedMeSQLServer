{
  "name": "who-changed-that-sql-server",
  "version": "1.0.0",
  "description": "Who Changed That â€” SQL Server Version. A Kovoco Inc tool to audit changes on a single SQL Server object and identify who made changes and from where.",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux"
  },
  "build": {
    "appId": "com.kovoco.whochangedthat.sqlserver",
    "productName": "Who Changed That - SQL Server Version",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "src/assets/icon.ico"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "keywords": ["sql-server", "audit", "database", "change-tracking"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "mssql": "^10.0.2"
  }
}
